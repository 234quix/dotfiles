init_python3_data_science_virtenv () {
   echo "Setting up virtual environment"
   python -m venv .venv
   echo "Activating virtual environment"
   source .venv/bin/activate
   echo "Installing packages..."
   pip install setuptools pynvim pandas jupyter matplotlib sklearn jupytext python-language-server jedi PySide2
   }
